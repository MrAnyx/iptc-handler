{% extends "base.html.twig" %}

{% block body %}

	<div style="display: grid;  grid-template-columns: repeat(4, 1fr); grid-gap: 10px;">
		{% for image in images %}
			<a href="{{ path('app_details', {imageName: image.filename}) }}">
				<img src="{{image.path}}" style="width: 100%; height: 100%; object-fit: cover;">
			</a>
		{% endfor %}
	</div>

{% endblock %}
